@page
@model GroupIS1024.Pages.NewsModel
@{
    List<Sport> sports = (List<Sport>) ViewData["Sports"];
}
<div class="text-center">
    <table>
        <tr>
            <th>Column 1</th>
            <th>Column 2</th>
            <th>Column 3</th>
            <th>Column 4</th>
            <th>Column 5</th>

        </tr>
    @{
        foreach (Sport sport in sports)
        {
                <tr>
                    <td>@sport.Author</td>
                    <td>@sport.Content</td>
                    <td>@sport.Title</td>
                    <td>@sport.Time</td>
                    <td>@sport.ReadMoreUrl</td>

                </tr>
        }
    }
</table>
</div>