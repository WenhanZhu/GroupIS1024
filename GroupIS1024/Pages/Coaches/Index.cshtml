@page
@model GroupIS1024.Pages.Coaches.IndexModel

@{
    ViewData["Title"] = "Index";
}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
   "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd"><html xmlns="http://www.w3.org/1999/xhtml" lang="en" xml:lang="en">
<style>
        .cards {
      padding: 16px;
      text-align: center;
      background-color: #0064D5 ;
      color: white;
      border-radius:20px;
    }
    
        .column {
      float: left;
      width: 25%;
      padding: 0 5px;
    }

    .row {margin: 0 -5px;}

    /* Clear floats after the columns */
    .row:after {
      content: "";
      display: table;
      clear: both;
    }
        {font-family: Arial, Helvetica, sans-serif;}
    * {box-sizing: border-box;}

    input[type=text], select, textarea {
      width: 100%;
      padding: 12px;
      border: 1px solid #ccc;
      border-radius: 4px;
      box-sizing: border-box;
      margin-top: 6px;
      margin-bottom: 16px;
      resize: vertical;
    }

    input[type=submit] {
      background-color:#0064D5 ;
      color: white;
      padding: 12px 20px;
      border: none;
      border-radius: 4px;
      cursor: pointer;
      margin-bottom:50px
    }

    input[type=submit]:hover {
      background-color:#0064D5 ;
    }
         .card {
        position: relative;
        display: flex;
        flex-direction: column;
        min-width: 0;
        word-wrap: break-word;
        background-color: #fff;
        background-clip: border-box;
        border-radius: 0.25rem;
        border:0px;
    }
<div class="container">
    <div class="header">
        <div class="main-header">
            <h1>Fantasy Managers List</h1>
        </div>
        <div class="sub-header">
            <h3>Find Managers here</h3>
        </div>
    </div>

    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Coach[0].FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Coach[0].LastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Coach[0].Username)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Coach[0].State)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Coach[0].Memberstatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Coach[0].Team)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Coach)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Username)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.State)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Memberstatus)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Team.TeamId)
                </td>
                <td>
                    <a asp-page="./Edit" asp-route-id="@item.CoachId">Edit</a> |
                    <a asp-page="./Details" asp-route-id="@item.CoachId">Details</a> |
                    <a asp-page="./Delete" asp-route-id="@item.CoachId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
    </table>
</div>